<script is:inline>
// let increment = 0;
let scroll;

const paths = document.querySelectorAll("svg line");

for (i = 0; i < paths.length; i++) {
    paths[i].style.strokeDashoffset = `${scroll * 1.5}px`;

    paths[i].style.strokeDasharray = `${Math.random() * 120 + 60}rem`;
}

window.addEventListener("scroll", function () {
    scroll = window.scrollY;

    for (i = 0; i < paths.length; i++) {
        paths[i].style.strokeDashoffset = `${scroll * 2}px`;
    }
});

// setTimeout(() => {
// }, 200);

document.onreadystatechange = () => {
    if (document.readyState === "complete") {
        for (i = 0; i < paths.length; i++) {
            paths[i].style.strokeDashoffset = `${Math.random() * 120 + 60}rem`;
        }

        // document.querySelector('.logo_outline').style.opacity = 1
    }
}
</script>
